{
    "name": "collingmedia/infusionsoft",
    "description": "Infusionsoft PHP API - an easy to use version of the PHP Infusionsoft API using GuzzleHTTP.",
    "keywords": [
        "infusionsoft",
        "php",
        "sdk"
    ],
    "homepage": "http://collingmedia.com",
    "license": "",
    "authors": [
        {
            "name": "Colling Media",
            "homepage": "https://github.com/colling-media"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": { "CollingMedia\\Infusionsoft\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "CollingMedia\\Infusionsoft\\" : "src/" }
    }
}
